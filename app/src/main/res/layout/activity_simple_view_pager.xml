<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.yenhenchia.projectpractice.ViewPager.SimpleViewPagerActivity">

    <include layout="@layout/basetoolbar" />

    <com.example.yenhenchia.projectpractice.ViewPager.BaseViewPager
        android:id="@+id/BaseViewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="60dp"
        android:clipToPadding="false">

    </com.example.yenhenchia.projectpractice.ViewPager.BaseViewPager>
</LinearLayout>
